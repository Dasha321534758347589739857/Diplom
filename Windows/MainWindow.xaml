                                        <RotateTransform3D>
                                            <RotateTransform3D.Rotation>
                                                <AxisAngleRotation3D x:Name="rotate" Axis="0 1 0" />
                                            </RotateTransform3D.Rotation>
                                        </RotateTransform3D>
                                    </ModelVisual3D.Transform>
                                </ModelVisual3D>
                            </Viewport3D.Children>
                        </Viewport3D>-->

                    </TabItem>
                    <!--<TabItem
                        Foreground="Black"
      Header="Радиусы поражения на карте">
                         </TabItem>-->

                </TabControl>
            </materialDesign:Card>
        </Grid>

        <Grid Grid.Column="3" Grid.Row="1" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="8*" />

                <RowDefinition Height="2*" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                
                    <Label Margin ="5" FontSize="20" Background="White" BorderThickness="4" Foreground="Black"  >
                    Результат:
                </Label>
                   
            </Grid>
            <Grid Grid.Row="1">

                <Grid >

                    <StackPanel Margin="10">

                        <Label
   
                           Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
                           VerticalContentAlignment="Center" 
                             HorizontalContentAlignment="Center"
                             FontSize="15"
                            
                           >
                            Расстояние от эпицентра взрыва, м:
                        </Label>
                        <TextBox
                              Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                              VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                              IsReadOnly="True" 
                              Text="{Binding DistanceE}">

                        </TextBox>
                        <Label
   
    Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
    VerticalContentAlignment="Center" 
      HorizontalContentAlignment="Center"
      FontSize="15"
    >
                            Вероятность потери ориентации, %:
                        </Label>
                        <TextBox
                                 
                                 Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                 VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                 IsReadOnly="True" 
                                 Text="{Binding Orientation}">

                        </TextBox>
                        <Label
   
Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
VerticalContentAlignment="Center" 
  HorizontalContentAlignment="Center"
  FontSize="15"
>
                            Разрыв барабанных перепонок, %:
                        </Label>
                        <TextBox
                                Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                IsReadOnly="True" 
                                Text="{Binding Eardrumms}">

                        </TextBox>
                        <Label
   
Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
VerticalContentAlignment="Center" 
  HorizontalContentAlignment="Center"
  FontSize="15"
>
                            Отброс ударной волной, %:
                        </Label>
                        <TextBox
                                Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                IsReadOnly="True" 
                                Text="{Binding ThrowWay}">

                        </TextBox>
                        <Label
   
Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
VerticalContentAlignment="Center" 
  HorizontalContentAlignment="Center"
  FontSize="15"
>
                            Поражение органов дыхания:
                        </Label>
                        <TextBox
                                
                                Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                IsReadOnly="True" 
                                Text="{Binding ResperatorsOrgan}">

                        </TextBox>
                        <Label
   
Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
VerticalContentAlignment="Center" 
  HorizontalContentAlignment="Center"
  FontSize="15"
>
                            Степень поражения:
                        </Label>
                        <TextBox
                                Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                IsReadOnly="True" 
                                Text="{Binding DegreeOfDamage}">

                        </TextBox>
                        <Label
   
Grid.Row="0" Grid.Column="1"  VerticalAlignment="Center"
VerticalContentAlignment="Center" 
  HorizontalContentAlignment="Center"
  FontSize="15"
>
                            Избыточное давление, кПа:
                        </Label>
                        <TextBox
                                Grid.Row="0" Grid.Column="1" Margin="7" VerticalAlignment="Center"
                                VerticalContentAlignment="Center" 
                                HorizontalContentAlignment="Center"
                                BorderBrush="#42b445"
                                BorderThickness="1"
                                IsReadOnly="True" 
                                Text="{Binding Pressure}">

                        </TextBox>

                    </StackPanel>
                </Grid>

            </Grid>
            <Grid Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*" />
                    <RowDefinition Height="1*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Viewbox HorizontalAlignment="Center" VerticalAlignment="Center" >
                        <Button
                             Command="{Binding SaveResCommand}"
                             Margin="10"
                             Background="White" BorderBrush="#42b445 " Content="Сохранить результаты"
                             Foreground="Black">
                        </Button>
                    </Viewbox>
                </Grid>
                <Grid Grid.Row="1">
                    <Viewbox HorizontalAlignment="Center" VerticalAlignment="Center" >

                        <Button
                            Command="{Binding SaveParCommand}"
                            Margin="10"
                            Background="White" BorderBrush="#42b445 " Content="Сохранить параметры"
                            Foreground="Black">
                        </Button>
                    </Viewbox>
                </Grid>
            </Grid>

        </Grid>
    </Grid>
</Window>
